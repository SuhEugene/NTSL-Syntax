{
  "name": "ntsl-lang",
  "displayName": "NTSL Syntax",
  "description": "Syntax for NTSL language of SpaceStation13 game",
  "author": "SuhEugene",
  "license": "ISC",
  "version": "1.0.1",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "icon": "ntsl.png",
  "publisher": "suheugene",
  "repository": {
    "type": "git",
    "url": "https://github.com/SuhEugene/NTSL-Syntax"
  },
  "contributes": {
    "languages": [
      {
        "id": "ntsl",
        "aliases": [
          "NTSL",
          "ntsl"
        ],
        "extensions": [
          ".ntsl",
          ".NTSL"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "ntsl",
        "scopeName": "source.ntsl",
        "path": "./syntaxes/ntsl.tmLanguage.json"
      }
    ]
  }
}
